{
  "name": "@karlorz/react-image-annotate",
  "version": "1.9.3",
  "type": "module",
  "description": "A powerful React component for image and video annotation with bounding boxes, polygons, points, and more",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": "karlorz",
  "license": "MIT",
