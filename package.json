  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://karlorz.github.io/react-image-annotate",
  "repository": {
    "type": "git",
    "url": "https://github.com/karlorz/react-image-annotate.git"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build && cp ./package.json ./dist/package.json && cp ./README.md ./dist/README.md",
    "preview": "vite preview",
    "dist": "npm run build && cd dist && npm publish",
    "storybook": "storybook dev -p 9090",
    "build-storybook": "storybook build",
    "build:gh-pages": "vite build --mode production && mkdir -p dist/demo && cp dist/index.html dist/demo/index.html",
    "gh-pages": "npm run build:gh-pages && gh-pages -d dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:test": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@storybook/addon-docs": "^10.0.4",
    "@storybook/addon-links": "^10.0.4",
    "@storybook/react-vite": "^10.0.4",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.2.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.4.2",
    "storybook": "^10.0.4",
    "vite": "^7.1.12",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^4.0.7"
  },
  "prettier": {
    "semi": false
  }
